#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/simonsmyth/Projects/yarnify/node_modules/.pnpm/ts-jest@28.0.8_@babel+core@7.27.1_@jest+types@28.1.3_babel-jest@28.1.3_@babel+core@7.27_37e14248f76d240d8d2c25bd260734f6/node_modules/ts-jest/node_modules:/Users/simonsmyth/Projects/yarnify/node_modules/.pnpm/ts-jest@28.0.8_@babel+core@7.27.1_@jest+types@28.1.3_babel-jest@28.1.3_@babel+core@7.27_37e14248f76d240d8d2c25bd260734f6/node_modules:/Users/simonsmyth/Projects/yarnify/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/simonsmyth/Projects/yarnify/node_modules/.pnpm/ts-jest@28.0.8_@babel+core@7.27.1_@jest+types@28.1.3_babel-jest@28.1.3_@babel+core@7.27_37e14248f76d240d8d2c25bd260734f6/node_modules/ts-jest/node_modules:/Users/simonsmyth/Projects/yarnify/node_modules/.pnpm/ts-jest@28.0.8_@babel+core@7.27.1_@jest+types@28.1.3_babel-jest@28.1.3_@babel+core@7.27_37e14248f76d240d8d2c25bd260734f6/node_modules:/Users/simonsmyth/Projects/yarnify/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
