#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/simonsmyth/Projects/yarnify/node_modules/.pnpm/prisma-json-types-generator@3.4.2_prisma@6.8.2_typescript@4.9.5__typescript@4.9.5/node_modules/prisma-json-types-generator/node_modules:/Users/simonsmyth/Projects/yarnify/node_modules/.pnpm/prisma-json-types-generator@3.4.2_prisma@6.8.2_typescript@4.9.5__typescript@4.9.5/node_modules:/Users/simonsmyth/Projects/yarnify/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/simonsmyth/Projects/yarnify/node_modules/.pnpm/prisma-json-types-generator@3.4.2_prisma@6.8.2_typescript@4.9.5__typescript@4.9.5/node_modules/prisma-json-types-generator/node_modules:/Users/simonsmyth/Projects/yarnify/node_modules/.pnpm/prisma-json-types-generator@3.4.2_prisma@6.8.2_typescript@4.9.5__typescript@4.9.5/node_modules:/Users/simonsmyth/Projects/yarnify/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../prisma-json-types-generator/index.js" "$@"
else
  exec node  "$basedir/../prisma-json-types-generator/index.js" "$@"
fi
