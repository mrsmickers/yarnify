#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/simon/projects/yarnify/node_modules/.pnpm/jest@28.1.3_@types+node@16.18.126_ts-node@10.9.2_@swc+core@1.11.29_@types+node@16.18.126_typescript@4.9.5_/node_modules/jest/bin/node_modules:/home/simon/projects/yarnify/node_modules/.pnpm/jest@28.1.3_@types+node@16.18.126_ts-node@10.9.2_@swc+core@1.11.29_@types+node@16.18.126_typescript@4.9.5_/node_modules/jest/node_modules:/home/simon/projects/yarnify/node_modules/.pnpm/jest@28.1.3_@types+node@16.18.126_ts-node@10.9.2_@swc+core@1.11.29_@types+node@16.18.126_typescript@4.9.5_/node_modules:/home/simon/projects/yarnify/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/simon/projects/yarnify/node_modules/.pnpm/jest@28.1.3_@types+node@16.18.126_ts-node@10.9.2_@swc+core@1.11.29_@types+node@16.18.126_typescript@4.9.5_/node_modules/jest/bin/node_modules:/home/simon/projects/yarnify/node_modules/.pnpm/jest@28.1.3_@types+node@16.18.126_ts-node@10.9.2_@swc+core@1.11.29_@types+node@16.18.126_typescript@4.9.5_/node_modules/jest/node_modules:/home/simon/projects/yarnify/node_modules/.pnpm/jest@28.1.3_@types+node@16.18.126_ts-node@10.9.2_@swc+core@1.11.29_@types+node@16.18.126_typescript@4.9.5_/node_modules:/home/simon/projects/yarnify/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../jest/bin/jest.js" "$@"
fi
