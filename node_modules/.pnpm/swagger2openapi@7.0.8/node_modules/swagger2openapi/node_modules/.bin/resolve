#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/simon/projects/yarnify/node_modules/.pnpm/oas-resolver@2.5.6/node_modules/oas-resolver/node_modules:/home/simon/projects/yarnify/node_modules/.pnpm/oas-resolver@2.5.6/node_modules:/home/simon/projects/yarnify/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/simon/projects/yarnify/node_modules/.pnpm/oas-resolver@2.5.6/node_modules/oas-resolver/node_modules:/home/simon/projects/yarnify/node_modules/.pnpm/oas-resolver@2.5.6/node_modules:/home/simon/projects/yarnify/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../oas-resolver@2.5.6/node_modules/oas-resolver/resolve.js" "$@"
else
  exec node  "$basedir/../../../../../oas-resolver@2.5.6/node_modules/oas-resolver/resolve.js" "$@"
fi
